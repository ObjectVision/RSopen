container BAGSloopNieuwbouwWonen
{
	container Sloop :=
		for_each_nedv(
			Vastgoed/WP4/name,
			'recollect_by_cond(AdminDomain/IsCompactedDomain
				, value(
					BaseData/StartState/BAG_nieuwbouw/CalcNieuwbouw_perDoel/woon/WegVBO_TussenModelenRecentJaar_PerDoel/count_Per_CompactedAdminDomain_WP4/'+Vastgoed/WP4/name+'
					+ BaseData/StartState/BAG_nieuwbouw/CalcNieuwbouw_perDoel/woon/WegVBO_NaRecentJaar_PerDoel/count_Per_CompactedAdminDomain_WP4/'+Vastgoed/WP4/name+'
				, Woning))',
			AdminDomain, Woning
		)
	{
		attribute<Woning> Totaal (AdminDomain) := = 'add('+asItemList(Vastgoed/WP4/name)+')';
	}
	
	container Nieuwbouw :=
		for_each_nedv(
			Vastgoed/WP4/name,
			'recollect_by_cond(AdminDomain/IsCompactedDomain, '
				'value('
					  'BaseData/StartState/BAG_nieuwbouw/CalcNieuwbouw_perDoel/woon/NieuwVBO_TussenModelenRecentJaar_PerDoel/count_Per_CompactedAdminDomain_WP4/'+Vastgoed/WP4/name+ // nieuwe voorraad vbo's tussen model/recent Year
					'+ BaseData/StartState/BAG_nieuwbouw/CalcNieuwbouw_perDoel/woon/NieuwVBO_NaRecentJaarWelActief_PerDoel/count_Per_CompactedAdminDomain_WP4/'+Vastgoed/WP4/name+ // nieuwe voorraad vbo's tussen model/recent Year
					'+ BaseData/StartState/BAG_nieuwbouw/CalcNieuwbouw_perDoel/woon/NieuwVBO_NaRecentJaar_PerDoel/count_Per_CompactedAdminDomain_WP4/'+Vastgoed/WP4/name+                     // nieuwe VBOs in een nieuw bouw pand, na recent Year
				',Woning))',
			AdminDomain, Woning
		)
	{
		attribute<Woning> Totaal (AdminDomain) := = 'add('+asItemList(Vastgoed/WP4/name)+')';
	}
	
