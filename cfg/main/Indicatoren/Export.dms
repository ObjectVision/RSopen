container Export : using = "classifications/actor"
{
		// Verandering netto opbrengsten in euroâ€™s (bedrijfwinst) tussen basisjaar en 2050:
			// Per ha totaal landbouwgebied (sector)
			// Opgeteld voor Friesland, in landbouwgebied
		// CO2 emissies, mton en eur, via prijs:
			// Nu, per jaar,
		// Bereikbaarheid groen, incl geheel vernatte landbouw)
		// Bereikbaarheid open water (geen zee, wel retentie)
		// transitie matrix

	attribute<Ha>   Basisjaar_Areaal_perLUMT       (LU_ModelType) := pcount(Indicatoren/Basisjaar/Landgebruikskaart/Result_SA)[float32] * rdc_25m/NrHaPerCell; 
	attribute<Ha>   Y2050_Areaal_perLUMT           (LU_ModelType) := pcount(Indicatoren/Y2050/Landgebruikskaart/Result_SA)[float32] * rdc_25m/NrHaPerCell; 
	attribute<Ha>   Verandering_Areaal_perLUMT     (LU_ModelType) := Y2050_Areaal_perLUMT - Basisjaar_Areaal_perLUMT;
	
	container Basisjaar_NettoOpbrengsten_perLUMT :=
		for_each_nedv(
			LU_ModelType/label
			, LU_ModelType/IsLandbouw 
					? 'Indicatoren/Basisjaar/Landgebruikskaart/Result_SA == LU_ModelType/v/'+LU_ModelType/label+'
						? VariantData/Suitability/Y2050/Landbouw/per_ModelType/'+LU_ModelType/subsector_name+'/NetRevenue[rdc_25m/rdc_100m_rel] * rdc_25m/NrHaPerCell * 1[jaar] / 1[ha]
						: 0[Eur_Ha]'
					: 'const(0[Eur_Ha], rdc_25m)'
			, rdc_25m
			, Eur_Ha
		)
	{
		attribute<Eur_Ha> Totaal_Landbouw (rdc_25m) := ='add('+AsItemList(LU_ModelType/label)+')';
		parameter<Eur_ha> Gemiddeld_perHa           := sum(Totaal_Landbouw) / sum(LU_ModelType/IsLandbouw[Indicatoren/Basisjaar/Landgebruikskaart/Result_SA][float32]) * rdc_25m/NrHaPerCell / 1[ha];
	}
	
	container Y2050_NettoOpbrengsten_perLUMT :=
		for_each_nedv(
			LU_ModelType/label
			, LU_ModelType/IsLandbouw 
					? 'Indicatoren/Y2050/Stand/Subsector_rel == rlookup(LU_ModelType/v/'+LU_ModelType/label+', Classifications/Actor/Sector/XSubsector/LU_ModelType_rel)
						? VariantData/Suitability/Y2050/Landbouw/per_ModelType/'+LU_ModelType/subsector_name+'/NetRevenue[rdc_25m/rdc_100m_rel] * 1[jaar]
						: 0[Eur_Ha]'
					: 'const(0[Eur_Ha], rdc_25m)'
			, rdc_25m
			, Eur_Ha
		)
	{
		attribute<Eur_Ha> Totaal_Landbouw (rdc_25m) := ='add('+AsItemList(LU_ModelType/label)+')';
		parameter<Eur_ha> Gemiddeld_perHa           := sum(Totaal_Landbouw) / sum(LU_ModelType/IsLandbouw[Indicatoren/Y2050/Landgebruikskaart/Result_SA][float32]) * rdc_25m/NrHaPerCell / 1[ha];
	}

