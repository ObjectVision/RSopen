container Claims :=
	for_each_ne(
		Time/Zichtjaar/name
		,'Templates/Claims/Zichtjaar_T('+string(id(Time/Zichtjaar))+'[Time/Zichtjaar], Claims, CaseClassifications)'
	), using = "Classifications/Time"
{
}
