unit<uint8> VariantK: NrOfRows = 3  
{                                                                         // BusinessAsUsual                  , Intensiveren (RV23; MO, OO?)      , Transformeren (RV23; GL, GR?)
	attribute<string_t> name:                                              ['BAU'                             ,'Intensiveren'                     ,'Transformeren'                    ];
	attribute<string_t> Label := union_data                                (.,'BusinessAsUsual'               ,'Intensiveren'                     ,'Transformeren'                    ), DialogType = "LabelText";

///////////////////////////////////////////////////////////////////////////////////// Wonen /////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

	attribute<string>    OntwikkelPakketVariant:                           [ 'Default'                        ,'Intensiveren'                     ,'Transformeren'                    ];
	
	// Geldende restricties resp. stimuli: alleen (zeer) hard, of ook middel, zacht?
	attribute<string_t>  RestrictiesVariant_Wonen:                         ['ZeerHardHardMiddel'              ,'ZeerHardHardMiddel'               ,'ZeerHardHardMiddelZacht'           ]; //fka Restricties_ref
		attribute<string_t>  StimuliVariant_Wonen:                             ['HardMiddel'                      ,'HardMiddel'                       ,'HardMiddel'                       ]; //fka Stimuli_ref

	// selectie van BBG-klassen die voor deze sector als ReedsBenut gelden
	attribute<string>    ReedsBenutVariant_Wonen:                          [ 'Default'                        ,'Intensiveren'                     ,'Transformeren'                    ];

	attribute<Woning_ha> MaxDichtheidInMooiLandschap:                      [100                               , 40                                , 35                                ];
	attribute<Woning_ha> MaxDichtheidInZeerMooiLandschap:                  [ 50                               , 35                                , 30                                ];
	
	//EG=Eengezins, MG=Meergezins.
		attribute<Woning_ha> MinimumDichtheid_EG_WLO_hoog:                     [ 20                               , 20                                , 20                                ], Descr = "Minimum dichtheid voor WoningType Eengezins (EG) in scenario WLO Hoog. ";
		attribute<Woning_ha> MinimumDichtheid_MG_WLO_hoog:                     [ 40                               , 40                                , 40                                ], Descr = "Minimum dichtheid voor WoningType Meergezins (MG) in scenario WLO Hoog";
		attribute<Woning_ha> MinimumDichtheid_EG_WLO_laag:                     [ 15                               , 15                                , 15                                ], Descr = "Minimum dichtheid voor WoningType Eengezins (EG) in scenario WLO Laag";
		attribute<Woning_ha> MinimumDichtheid_MG_WLO_laag:                     [ 35                               , 35                                , 35                                ], Descr = "Minimum dichtheid voor WoningType Meergezins (MG) in scenario WLO Laag";

	attribute<bool_t>    HoogbouwToegestaanInMooiLandschap:                [ TRUE                             , FALSE                             , FALSE                             ];
	attribute<bool_t>    HoogbouwToegestaanInZeerMooiLandschap:            [ FALSE                            , FALSE                             , FALSE                             ];
	attribute<bool_t>    TinyHousesToegestaan:                             [ FALSE                            , TRUE                              , FALSE                             ];
	attribute<bool_t>    SuperStedelijkToegestaan:                         [ TRUE                             , TRUE                              , TRUE                              ];
	
	// Generieke (niet-locatiespecifieke) Factor, te vermenigvuldigen met waargenomen Woningdichtheden, t.b.v. het bepalen van de maximale dichtheid waarin gebouwd mag worden. Hoe hoger de factor, hoe hoger het maximum.
	attribute<float32_t> DichtheidFactorGeneriek:                          [1.00                              ,1.20                               ,1.20                               ];
	
	// Fractie waarmee de Woningdichtheid minimaal moet toenemen in AllocDomain
	attribute<float32_t> MinimumDichtheidToenameWonen:                     [0.10                              ,0.05                               ,0.10                               ], Descr = "Fractie waarmee de Woningdichtheid minimaal moet toenemen in AllocDomain";
	
	// Mate waarin de verharding van nieuwbouwlocaties in de Varianten hoger is dan de default.
	attribute<float32_t> VerhardingTovDefault:                             [1.00                              ,1.00                               ,0.50                               ], Descr = "";
	
///////////////////////////////////////////////////////////////////////////////////// Werken ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

	// selectie van BBG-klassen die voor deze sector als ReedsBenut gelden
	attribute<string>    ReedsBenutVariant_Werken:                         [ 'Default'                        ,'Default'                          ,'Default'                          ], Descr = "";

	// Geldende restricties resp. stimuli: alleen (zeer) hard, of ook middel, zacht?
	attribute<string_t>  RestrictiesVariant_Werken:                        ['ZeerHardHardMiddel'              ,'ZeerHardHardMiddel'               ,'ZeerHardHardMiddelZacht'          ], Descr = ""; //fka Restricties_ref
		attribute<string_t>  StimuliVariant_Werken:                            ['HardMiddel'                      ,'HardMiddel'                       ,'HardMiddel'                       ], Descr = ""; //fka Stimuli_ref

	// Fractie waarmee de Woningdichtheid minimaal moet toenemen in AllocDomain (100m cel)
	attribute<float32_t> MinimumDichtheidToenameWerken:                    [0.10                              ,0.10                               ,0.10                               ], Descr = ""; // om verdunning te voorkomen (niet te sturen met huidge versie van suit maps waar geen transitiekosten in zitten)
	// Minimale fractie van een subsector, Detailhandel bijvoorbeeld, die al aanwezig moet zijn in een gridcel
	attribute<float32_t> MinimumSubsectorShare:                            [0.01                              ,0.01                               ,0.01                               ], Descr = "";
	
	// De zgn. Kantoorcoefficioent en de groeifactor die daaruit kan worden afgeleid. Bron: expert judgement PBL. Default: /ModelParameters/m2BVO_job_Kantoor_PreCORONA
	attribute<m2BVO_job> m2BVO_job_Kantoor:                                [17.5                              ,15.0                               ,15.0                               ], Descr = "";
	attribute<float32_t> m2BVO_job_Kantoor_groeifactor := ((m2BVO_job_Kantoor / /ModelParameters/werken/m2BVO_job_Kantoor_PreCORONA) - 1f) / 1f;
	
	// Groeifactor t.a.v. m2pandFootprint_baan werken
	// Aanpassing van werkdichtheid op bestaand werkgebied gebeurt hier: /VariantData/Allocatie/Zichtjaren/Y2030/StateVoorAllocatie/Werken
	// Aanpassing van werkdichtheid op nieuw werkgebied gebeurt hier: /VariantData/Dichtheid/Per_AllocDomain/Y2030/Src/Werken/Job_Perm2PandFootprint/Subsectoren
	container Groeifactor := ModelParameters/Werken/Groeifactor;
																		  // BAU                               ,Intensiveren                      ,Transformeren                               
	attribute<jaarlijks> Nijverheid             := union_data             (.,Groeifactor/Nijverheid            ,Groeifactor/Nijverheid            ,Groeifactor/Nijverheid             );
	attribute<jaarlijks> Logistiek              := union_data             (.,Groeifactor/Logistiek             ,Groeifactor/Logistiek             ,Groeifactor/Logistiek              );
	attribute<jaarlijks> Detailhandel           := union_data             (.,Groeifactor/Detailhandel          ,Groeifactor/Detailhandel          ,Groeifactor/Detailhandel           );
	attribute<jaarlijks> Ov_consumentendiensten := union_data             (.,Groeifactor/Ov_consumentendiensten,Groeifactor/Ov_consumentendiensten,Groeifactor/Ov_consumentendiensten );
	attribute<jaarlijks> Zak_dienstverlening    := union_data             (.,Groeifactor/Zak_dienstverlening   ,Groeifactor/Zak_dienstverlening   ,Groeifactor/Zak_dienstverlening    );
	attribute<jaarlijks> Overheid_kw_diensten   := union_data             (.,Groeifactor/Overheid_kw_diensten  ,Groeifactor/Overheid_kw_diensten  ,Groeifactor/Overheid_kw_diensten   );

///////////////////////////////////////////////////////////////////////////////// Wonen/Werken ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
	
	attribute<string_t>  ClaimVariantName:                                 ['BAU'                             ,'BAU'                              ,'BAU'                    ], Descr = "Welke variant uit de TIGRIS claims moet worden gekozen";
	
	// Diverse locatiespecifieke factoren, te vermenigvuldigen met waargenomen dichtheden, t.b.v. het bepalen van de maximale dichtheid waarin gebouwd mag worden. Hoe hoger de factor, hoe hoger het maximum.
	attribute<float32_t> DichtheidFactorOV_EG:                             [1.10                              ,2.00                               ,2.00                               ];
	attribute<float32_t> DichtheidFactorOV_MG:                             [1.50                              ,2.00                               ,2.00                               ];
	attribute<float32_t> DichtheidFactorOV_NietWonen:                      [1.50                              ,2.00                               ,2.00                               ];
	attribute<float32_t> DichtheidFactorWater_EG:                          [1.10                              ,2.00                               ,2.00                               ];
	attribute<float32_t> DichtheidFactorWater_MG:                          [1.50                              ,2.00                               ,2.00                               ];
	attribute<float32_t> DichtheidFactorWater_NietWonen:                   [1.50                              ,2.00                               ,2.00                               ];
	
	attribute<float32_t> DichtheidFactorBevKern100kPlus_EGWLO_hoog:        [1.20                              ,1.20                               ,1.25                               ];
	attribute<float32_t> DichtheidFactorBevKern100kPlus_MGWLO_hoog:        [1.20                              ,1.20                               ,1.25                               ];
	attribute<float32_t> DichtheidFactorBevKern100kPlus_NietWonenWLO_hoog: [1.20                              ,1.20                               ,1.25                               ];
	attribute<float32_t> DichtheidFactorBevKern20kPlus_EGWLO_hoog:         [1.10                              ,1.10                               ,1.15                               ];
	attribute<float32_t> DichtheidFactorBevKern20kPlus_MGWLO_hoog:         [1.10                              ,1.10                               ,1.15                               ];
	attribute<float32_t> DichtheidFactorBevKern20kPlus_NietWonenWLO_hoog:  [1.10                              ,1.10                               ,1.15                               ];
	attribute<float32_t> DichtheidFactorBevKern20kMin_EGWLO_hoog:          [1.10                              ,1.10                               ,1.10                               ];
	attribute<float32_t> DichtheidFactorBevKern20kMin_MGWLO_hoog:          [1.10                              ,1.10                               ,1.10                               ];
	attribute<float32_t> DichtheidFactorBevKern20kMin_NietWonenWLO_hoog:   [1.10                              ,1.10                               ,1.10                               ];
	
	attribute<float32_t> DichtheidFactorBevKern100kPlus_EGWLO_laag:        [1.10                              ,1.20                               ,1.25                               ];
	attribute<float32_t> DichtheidFactorBevKern100kPlus_MGWLO_laag:        [1.10                              ,1.20                               ,1.25                               ];
	attribute<float32_t> DichtheidFactorBevKern100kPlus_NietWonenWLO_laag: [1.10                              ,1.20                               ,1.25                               ];
	attribute<float32_t> DichtheidFactorBevKern20kPlus_EGWLO_laag:         [1.00                              ,1.10                               ,1.15                               ];
	attribute<float32_t> DichtheidFactorBevKern20kPlus_MGWLO_laag:         [1.00                              ,1.10                               ,1.15                               ];
	attribute<float32_t> DichtheidFactorBevKern20kPlus_NietWonenWLO_laag:  [1.00                              ,1.10                               ,1.15                               ];
	attribute<float32_t> DichtheidFactorBevKern20kMin_EGWLO_laag:          [1.00                              ,1.10                               ,1.10                               ];
	attribute<float32_t> DichtheidFactorBevKern20kMin_MGWLO_laag:          [1.00                              ,1.10                               ,1.10                               ];
	attribute<float32_t> DichtheidFactorBevKern20kMin_NietWonenWLO_laag:   [1.00                              ,1.10                               ,1.10                               ];
	
	// Minimale fractie van een gridcel (AllocDomain) die beschikbaar moet zijn voor bebouwing
	attribute<ha>        MinimalLandAvailability:                          [0.40                              ,0.10                               ,0.10                               ];
	
	// Het hoogste bouwjaar resp. energielabel van gebouwen die mogen worden gesloopt.
	attribute<string_t>     GeenSloopvanafBouwjaar_BuitenHardePlancapaciteit: ['1970'                         ,'1980'                             ,'1980'                             ], Descr = "Geen sloop toegestaan van woningen met een bouwjaar later dan opgegeven."; 
	attribute<Energielabel> GeenSloopvanafELabel := union_data                (., Energielabel/V/D               ,Energielabel/V/D                   ,Energielabel/V/C                   ), Descr = "Geen sloop toegestaan van woningen met een lager dan opgegeven label. Oftewel, als D dan mogen C en lager niet worden gesloopt."; 
	
	attribute<Eur>       MinimumExploitatieSaldo:                          [0                                 ,0                                  ,0                                  ];

																		   // BAU                             ,Intensiveren                       ,Transformeren                               
	attribute<bool_t>    NietBouwenInNatura2000Buffer:                     [FALSE                             , FALSE                              , TRUE                              ];
	attribute<bool_t>    NietBouwenVerVanOV:                               [FALSE                             ,FALSE                              ,FALSE                              ];
	attribute<bool_t>    NietBouwenOpZeehaven:                             [ TRUE                             , TRUE                              , TRUE                              ];
	attribute<bool_t>    NietBouwenWaarSlap:                               [FALSE                             , FALSE                              , TRUE                              ];
	attribute<bool_t>    NietBouwenWaarNat:                                [FALSE                             , FALSE                              , TRUE                              ]; // In WBS om (bouw)kosten te besparen
	attribute<bool_t>    NietBouwenWaarZettingsgevoelig:                   [FALSE                             , FALSE                              , TRUE                              ];
	
	// Aangepast bouwen, bijv op een terp, is een geaccepteerde maatregel
	attribute<bool_t>    MaatregelKanAangepastBouwenZijn:                  [FALSE                             , TRUE                              , TRUE                              ];
	
	attribute<bool_t>    NietBouwenInZeerMooiLandschapWonen:               [false                             ,false                              ,false                              ];
	attribute<bool_t>    NietBouwenInZeerMooiLandschapWerken:              [false                             ,false                              ,false                              ];
	attribute<bool_t>    NietBouwenInMooiLandschap:                        [false                             ,false                              ,false                              ];
	attribute<bool_t>    NietBouwenInNatuurorganisaties:                   [false                             , true                              , true                              ];
	attribute<bool_t>    NietBouwenInWaterschappen:                        [false                             , true                              , true                              ];
	attribute<bool_t>    BouwenInWoonlocatieNietVanGemeenteOfCorporatie:   [false                             ,False                              ,false                              ];
	
	// Buiten harde plancapaciteit zijn restricties sowieso geldig. Geldt dat ook binnen harde plancapaciteit?
	attribute<bool_t>    RestrictiesOokGeldigBinnenHardePlancapaciteit:    [FALSE                             ,FALSE                              , true                              ];
	
	// Worden harde resp. zachte plannen uitgezonderd in de zeefanalyse?
	attribute<bool_t>    HardePlannenGeldigInZeef:                         [ TRUE                             , TRUE                              , TRUE                              ];
	attribute<bool_t>    ZachtePlannenGeldigInZeef:                        [ TRUE                             , TRUE                              , TRUE                              ];

																		   // BAU                             ,Intensiveren OO                     ,Transformeren gr                              
	attribute<MaatregelK> GevaarRegimeZone0 := union_data                  (., MaatregelK/V/Bouwen            ,MaatregelK/V/Bouwen                ,MaatregelK/V/Bouwen                );
	attribute<MaatregelK> GevaarRegimeZone1 := union_data                  (., MaatregelK/V/Bouwen            ,MaatregelK/V/Bouwen                ,MaatregelK/V/Bouwen                );
	attribute<MaatregelK> GevaarRegimeZone2 := union_data                  (., MaatregelK/V/Bouwen            ,MaatregelK/V/Bouwen                ,MaatregelK/V/Maatregelen           );
	attribute<MaatregelK> GevaarRegimeZone3 := union_data                  (., MaatregelK/V/Bouwen            ,MaatregelK/V/Maatregelen           ,MaatregelK/V/Maatregelen           );
	attribute<MaatregelK> GevaarRegimeZone4 := union_data                  (., MaatregelK/V/Bouwen            ,MaatregelK/V/Maatregelen           ,MaatregelK/V/NietBouwen            );
	attribute<MaatregelK> GevaarRegimeZone5 := union_data                  (., MaatregelK/V/Bouwen            ,MaatregelK/V/Maatregelen           ,MaatregelK/V/NietBouwen            );
	
	// Welk delta scenario vormt het uitgangspunt qua klimaat?
	attribute<string_t>  KlimaatScenario_ref:                              ['Stoom2100'                       ,'Stoom2100'                        ,'Stoom2100'                        ];
	attribute<string_t>  ReserverenVoorRivierAfvoer_ref:                   ['Krap'                            ,'Krap'                             ,'Ruim'                             ];
	attribute<string>    Trede_Variant:                                    ['BAU'                             ,'BAU'                              ,'BAU'                              ];
		
	//LET OP! Deze wordt nu gegeneerd in de BaseData! Mochten we deze willen gaan wijzigen per variant, dan moeten we dit anders aanpakken.
	attribute<string>    ReedsBenutVariant_BGT:                            [ 'Default'                        ,'Default'                          ,'Default'                          ]; 
	
///////////////////////////////////////////////////////////////////////////////// Verblijfsrecreatie ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

	// selectie van BBG-klassen die voor deze sector als ReedsBenut gelden
	attribute<string>    ReedsBenutVariant_Verblijfsrecreatie:             [ 'Default'                        ,'Default'                          ,'Default'                          ];

	// Geldende restricties resp. stimuli: alleen (zeer) hard, of ook middel, zacht?
	attribute<string_t>  RestrictiesVariant_Verblijfsrecreatie:            ['ZeerHardHardMiddel'              ,'ZeerHardHardMiddel'               ,'ZeerHardHardMiddelZacht'               ]; //fka Restricties_ref
		attribute<string_t>  StimuliVariant_Verblijfsrecreatie:                ['HardMiddel'                      ,'HardMiddel'                       ,'HardMiddel'                       ]; //fka Stimuli_ref

	// Regionaal schaalniveau waarop de claims t.a.v. Verblijfsrecreatie worden gealloceerd
	attribute<string_t>  Verblijfsrecreatie_AllocRegio_name:               ['Provincie'                       ,'Provincie'                        ,'Provincie'                        ];
	
	attribute<float32_t> ClaimGroeiFactorTovTrendHa:                       [1.00                              ,1.50                               ,1.00                               ]; // te vermenigvuldingen met de groei tussen basisjaar en zichtjaar
	attribute<float32_t> ClaimGroeiFactorTovTrendObjecten:                 [1.00                              ,1.50                               ,1.00                               ]; // idem
	
	attribute<bool_t>    NietBouwenInZeerMooiLandschapVerblijfsrecreatie:  [FALSE                             ,FALSE                              , TRUE                              ];
	
/////////////////////////////////////////////////////////////////////////////////////// Zon ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

	// selectie van BBG-klassen die voor deze sector als ReedsBenut gelden
	attribute<string>    ReedsBenutVariant_Zon:                            [ 'Default'                        ,'Default'                          ,'Default'                          ];
	
	// Geldende restricties resp. stimuli: alleen (zeer) hard, of ook middel, zacht?
	attribute<string_t>  RestrictiesVariant_Zon:                           ['ZeerHardHardMiddel'              ,'ZeerHardHardMiddel'               ,'ZeerHardHardMiddel'               ]; //fka Restricties_ref
	attribute<string_t>  StimuliVariant_Zon:                               ['HardMiddel'                      ,'HardMiddel'                       ,'HardMiddel'                       ]; //fka Stimuli_ref
	
	// Gaan nieuw gealloceerde zonnevelden samen met bestaand groen?
	attribute<bool_t>    ZonCombinerenMetBestaandGroen:                    [FALSE                             , FALSE                             , FALSE                             ];
	
