/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
unit<uint8> VariantK: NrOfRows = 2  
{                                                                         // BusinessAsUsual                  , WBSA                              
	attribute<string_t> name:                                              ['BAU'                             ,'WBS'                             ];
	attribute<string_t> label := union_data                                (.,'BusinessAsUsual'               ,'WaterBodemSturend'               ), DialogType = "LabelText";

///////////////////////////////////////////////////////////////////////////////////// Wonen //////////////////////////////////////////////////////////////////////////////////////////////////

	attribute<string>    OntwikkelPakketVariant:                           [ 'Default'                        ,'Default'                          ];
	
	// Geldende restricties resp. stimuli: alleen (zeer) hard, of ook middel, zacht?
	attribute<string_t>  RestrictiesVariant_Wonen:                         ['ZeerHardHardMiddel'              ,'ZeerHardHardMiddel'               ]; //fka Restricties_ref
	attribute<string_t>  StimuliVariant_Wonen:                             ['HardMiddel'                      ,'HardMiddel'                       ]; //fka Stimuli_ref

	// selectie van BBG-klassen die voor deze sector als ReedsBenut gelden
	attribute<string>    ReedsBenutVariant_Wonen:                          [ 'Default'                        ,'Default'                          ];

	attribute<Woning_ha> MaxDichtheidInMooiLandschap:                      [100                               ,100                                ];
	attribute<Woning_ha> MaxDichtheidInZeerMooiLandschap:                  [ 50                               , 50                                ];
	
	attribute<Woning_ha> MinimumDichtheid_EG_WLO_hoog:                     [ 20                               , 20                                ];
	attribute<Woning_ha> MinimumDichtheid_MG_WLO_hoog:                     [ 40                               , 40                                ];
	attribute<Woning_ha> MinimumDichtheid_EG_WLO_laag:                     [ 15                               , 15                                ];
	attribute<Woning_ha> MinimumDichtheid_MG_WLO_laag:                     [ 35                               , 35                                ];

	attribute<bool_t>    HoogbouwToegestaanInMooiLandschap:                [ true                             , true                              ];
	attribute<bool_t>    HoogbouwToegestaanInZeerMooiLandschap:            [ false                            , false                             ];
	attribute<bool_t>    TinyHousesToegestaan:                             [ false                            , false                             ];
	attribute<bool_t>    SuperStedelijkToegestaan:                         [ true                             , true                              ];
	
	// Generieke (niet-locatiespecifieke) factor, te vermenigvuldigen met waargenomen woningdichtheden, t.b.v. het bepalen van de maximale dichtheid waarin gebouwd mag worden. Hoe hoger de factor, hoe hoger het maximum.
	attribute<float32_t> DichtheidFactorGeneriek:                          [1.00                              ,1.00                               ];
	// attribute<float32_t> DichtheidFactorGeneriekWLO_hoog:                  [1.10                              ,1.10                               ]; // generiek in de zin van: niet locatie(type)-specifiek
	// attribute<float32_t> DichtheidFactorGeneriekWLO_laag:                  [1.00                              ,1.00                               ]; // generiek in de zin van: niet locatie(type)-specifiek
	
	// Fractie waarmee de woningdichtheid minimaal moet toenemen in AllocDomain
	attribute<float32_t> MinimumDichtheidToenameWonen:                     [0.10                              ,0.10                               ];
	
	// Mate waarin de verharding van nieuwbouwlocaties in de varianten hoger is dan de default.
	attribute<float32_t> VerhardingTovDefault:                             [1.00                              ,0.50                               ];
	
///////////////////////////////////////////////////////////////////////////////////// Werken ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

	// selectie van BBG-klassen die voor deze sector als ReedsBenut gelden
	attribute<string>    ReedsBenutVariant_Werken:                         [ 'Default'                        ,'Default'                          ];

	// Geldende restricties resp. stimuli: alleen (zeer) hard, of ook middel, zacht?
	attribute<string_t>  RestrictiesVariant_Werken:                        ['ZeerHardHardMiddel'              ,'ZeerHardHardMiddel'               ]; //fka Restricties_ref
	attribute<string_t>  StimuliVariant_Werken:                            ['HardMiddel'                      ,'HardMiddel'                       ]; //fka Stimuli_ref

	// Fractie waarmee de woningdichtheid minimaal moet toenemen in AllocDomain
	attribute<float32_t> MinimumDichtheidToenameWerken:                    [0.10                              ,0.10                               ]; // om verdunning te voorkomen (niet te sturen met huidge versie van suit maps waar geen transitiekosten in zitten)
	// Minimale fractie van een subsector, detailhandel bijvoorbeeld, die al aanwezig moet zijn in een gridcel
	attribute<float32_t> MinimumSubSectorShare:                            [0.01                              ,0.01                               ];
	
	// De zgn. kantoorcoefficioent en de groeifactor die daaruit kan worden afgeleid. Bron: expert judgement PBL. Default: /ModelParameters/m2BVO_job_kantoor_PreCORONA
	attribute<m2BVO_job> m2BVO_job_kantoor:                                [17.5                              ,17.5                               ];
	attribute<float32_t> m2BVO_job_kantoor_groeifactor := ((m2BVO_job_kantoor / /ModelParameters/werken/m2BVO_job_kantoor_PreCORONA) - 1f) / 1f;
	
	// Groeifactor t.a.v. m2pandfootprint_baan werken
	// Aanpassing van werkdichtheid op bestaand werkgebied gebeurt hier: /VariantData/Allocatie/Zichtjaren/Y2030/StateVoorAllocatie/Werken
	// Aanpassing van werkdichtheid op nieuw werkgebied gebeurt hier: /VariantData/Dichtheid/Per_AllocDomain/Y2030/Src/Werken/Job_Perm2PandFootprint/Subsectoren
	container Groeifactor := ModelParameters/Werken/Groeifactor;
																		  // BAU                                 ,WBSA                               
	attribute<jaarlijks> Nijverheid             := union_data              (.,Groeifactor/Nijverheid             ,Groeifactor/Nijverheid             );
	attribute<jaarlijks> Logistiek              := union_data              (.,Groeifactor/Logistiek              ,Groeifactor/Logistiek              );
	attribute<jaarlijks> Detailhandel           := union_data              (.,Groeifactor/Detailhandel           ,Groeifactor/Detailhandel           );
	attribute<jaarlijks> Ov_consumentendiensten := union_data              (.,Groeifactor/Ov_consumentendiensten ,Groeifactor/Ov_consumentendiensten );
	attribute<jaarlijks> Zak_dienstverlening    := union_data              (.,Groeifactor/Zak_dienstverlening    ,Groeifactor/Zak_dienstverlening    );
	attribute<jaarlijks> Overheid_kw_diensten   := union_data              (.,Groeifactor/Overheid_kw_diensten   ,Groeifactor/Overheid_kw_diensten   );

///////////////////////////////////////////////////////////////////////////////// Wonen/Werken ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
	
	// Diverse locatiespecifieke factoren, te vermenigvuldigen met waargenomen dichtheden, t.b.v. het bepalen van de maximale dichtheid waarin gebouwd mag worden. Hoe hoger de factor, hoe hoger het maximum.
	attribute<float32_t> DichtheidFactorOV_EG:                             [1.10                              ,1.10                               ];
	attribute<float32_t> DichtheidFactorOV_MG:                             [1.50                              ,1.50                               ];
	attribute<float32_t> DichtheidFactorOV_NietWonen:                      [1.50                              ,1.50                               ];
	attribute<float32_t> DichtheidFactorWater_EG:                          [1.10                              ,1.10                               ];
	attribute<float32_t> DichtheidFactorWater_MG:                          [1.50                              ,1.50                               ];
	attribute<float32_t> DichtheidFactorWater_NietWonen:                   [1.50                              ,1.50                               ];
	
	attribute<float32_t> DichtheidFactorBevKern100kPlus_EGWLO_hoog:        [1.20                              ,1.20                               ];
	attribute<float32_t> DichtheidFactorBevKern100kPlus_MGWLO_hoog:        [1.20                              ,1.20                               ];
	attribute<float32_t> DichtheidFactorBevKern100kPlus_NietWonenWLO_hoog: [1.20                              ,1.20                               ];
	attribute<float32_t> DichtheidFactorBevKern20kPlus_EGWLO_hoog:         [1.10                              ,1.10                               ];
	attribute<float32_t> DichtheidFactorBevKern20kPlus_MGWLO_hoog:         [1.10                              ,1.10                               ];
	attribute<float32_t> DichtheidFactorBevKern20kPlus_NietWonenWLO_hoog:  [1.10                              ,1.10                               ];
	attribute<float32_t> DichtheidFactorBevKern20kMin_EGWLO_hoog:          [1.10                              ,1.10                               ];
	attribute<float32_t> DichtheidFactorBevKern20kMin_MGWLO_hoog:          [1.10                              ,1.10                               ];
	attribute<float32_t> DichtheidFactorBevKern20kMin_NietWonenWLO_hoog:   [1.10                              ,1.10                               ];
	
	attribute<float32_t> DichtheidFactorBevKern100kPlus_EGWLO_laag:        [1.10                              ,1.10                               ];
	attribute<float32_t> DichtheidFactorBevKern100kPlus_MGWLO_laag:        [1.10                              ,1.10                               ];
	attribute<float32_t> DichtheidFactorBevKern100kPlus_NietWonenWLO_laag: [1.10                              ,1.10                               ];
	attribute<float32_t> DichtheidFactorBevKern20kPlus_EGWLO_laag:         [1.00                              ,1.00                               ];
	attribute<float32_t> DichtheidFactorBevKern20kPlus_MGWLO_laag:         [1.00                              ,1.00                               ];
	attribute<float32_t> DichtheidFactorBevKern20kPlus_NietWonenWLO_laag:  [1.00                              ,1.00                               ];
	attribute<float32_t> DichtheidFactorBevKern20kMin_EGWLO_laag:          [1.00                              ,1.00                               ];
	attribute<float32_t> DichtheidFactorBevKern20kMin_MGWLO_laag:          [1.00                              ,1.00                               ];
	attribute<float32_t> DichtheidFactorBevKern20kMin_NietWonenWLO_laag:   [1.00                              ,1.00                               ];
	
	// Minimale fractie van een gridcel (AllocDomain) die beschikbaar moet zijn voor bebouwing
	attribute<ha>        MinimalLandAvailability:                          [0.10                              ,0.10                               ];
	
	// Het hoogste bouwjaar resp. energielabel van gebouwen die mogen worden gesloopt.
	attribute<string_t>  GeenSloopvanafBouwjaar_BuitenHardePlancapaciteit: ['1980'                            ,'1980'                             ], Descr = "Geen sloop toegestaan van woningen met een bouwjaar later dan opgegeven."; 
	attribute<Energielabel> GeenSloopvanafELabel := union_data             (., Energielabel/V/D               ,Energielabel/V/D                   ), Descr = "Geen sloop toegestaan van woningen met een lager dan opgegeven label. Oftewel, als D dan mogen C en lager niet worden gesloopt."; 
	
	attribute<Eur>       MinimumExploitatieSaldo:                          [0                                 ,0                                  ];

																		   // BAU                             ,WBSA                               
	attribute<bool_t>    NietBouwenInNatura2000Buffer:                     [false                             , true                              ];
	attribute<bool_t>    NietBouwenVerVanOV:                               [false                             ,false                              ];
	attribute<bool_t>    NietBouwenOpZeehaven:                             [ true                             , true                              ];
	attribute<bool_t>    NietBouwenWaarSlap:                               [false                             , true                              ];
	attribute<bool_t>    NietBouwenWaarNat:                                [false                             , true                              ]; // In WBS om (bouw)kosten te besparen
	attribute<bool_t>    NietBouwenWaarZettingsgevoelig:                   [false                             , true                              ];
	
	// Aangepast bouwen, bijv op een terp, is een geaccepteerde maatregel
	attribute<bool_t>    MaatregelKanAangepastBouwenZijn:                  [false                             , true                              ];
	
	attribute<bool_t>    NietBouwenInZeerMooiLandschapWonen:               [false                             ,false                              ];
	attribute<bool_t>    NietBouwenInZeerMooiLandschapWerken:              [false                             ,false                              ];
	attribute<bool_t>    NietBouwenInMooiLandschap:                        [false                             ,false                              ];
	attribute<bool_t>    NietBouwenInNatuurorganisaties:                   [ true                             , true                              ];
	attribute<bool_t>    NietBouwenInWaterschappen:                        [ true                             , true                              ];
	attribute<bool_t>    BouwenInWoonlocatieNietVanGemeenteOfCorporatie:   [false                             , false                              ];
	
	// Buiten harde plancapaciteit zijn restricties sowieso geldig. Geldt dat ook binnen harde plancapaciteit?
	attribute<bool_t>    RestrictiesOokGeldigBinnenHardePlancapaciteit:    [false                             ,false                              ];
	
	// Worden harde resp. zachte plannen uitgezonderd in de zeefanalyse?
	attribute<bool_t>    HardePlannenGeldigInZeef:                         [ true                             , true                              ];
	attribute<bool_t>    ZachtePlannenGeldigInZeef:                        [ true                             ,false                              ];

																		   // BAU                             ,WBSA                               
	attribute<MaatregelK> GevaarRegimeZone0 := union_data                  (., MaatregelK/V/Bouwen            ,MaatregelK/V/Bouwen                );
	attribute<MaatregelK> GevaarRegimeZone1 := union_data                  (., MaatregelK/V/Bouwen            ,MaatregelK/V/Bouwen                );
	attribute<MaatregelK> GevaarRegimeZone2 := union_data                  (., MaatregelK/V/Bouwen            ,MaatregelK/V/Maatregelen           );
	attribute<MaatregelK> GevaarRegimeZone3 := union_data                  (., MaatregelK/V/Bouwen            ,MaatregelK/V/Maatregelen           );
	attribute<MaatregelK> GevaarRegimeZone4 := union_data                  (., MaatregelK/V/Bouwen            ,MaatregelK/V/NietBouwen            );
	attribute<MaatregelK> GevaarRegimeZone5 := union_data                  (., MaatregelK/V/Bouwen            ,MaatregelK/V/NietBouwen            );
	
	// Welk delta scenario vormt het uitgangspunt qua klimaat?
	attribute<string_t>  Klimaatscenario_ref:                              ['Stoom2100'                       ,'Stoom2100'                        ];
	
	attribute<string>    Trede_variant:                                    ['BAU'                             ,'WBSA'                             ];
	
///////////////////////////////////////////////////////////////////////////////// Verblijfsrecreatie ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

	// selectie van BBG-klassen die voor deze sector als ReedsBenut gelden
	attribute<string>    ReedsBenutVariant_Verblijfsrecreatie:             [ 'Default'                        ,'Default'                          ];
	
	// Geldende restricties resp. stimuli: alleen (zeer) hard, of ook middel, zacht?
	attribute<string_t>  RestrictiesVariant_Verblijfsrecreatie:            ['ZeerHardHardMiddel'              ,'ZeerHardHardMiddel'               ]; //fka Restricties_ref
	attribute<string_t>  StimuliVariant_Verblijfsrecreatie:                ['HardMiddel'                      ,'HardMiddel'                       ]; //fka Stimuli_ref

	// Regionaal schaalniveau waarop de claims t.a.v. verblijfsrecreatie worden gealloceerd
	attribute<string_t>  Verblijfsrecreatie_AllocRegio_name:               ['Provincie'                       ,'Provincie'                        ];
	
	attribute<float32_t> ClaimGroeiFactorTovTrendHa:                       [1.00                              ,1.00                               ]; // te vermenigvuldingen met de groei tussen basisjaar en zichtjaar
	attribute<float32_t> ClaimGroeiFactorTovTrendObjecten:                 [1.00                              ,1.00                               ]; // idem
	
	attribute<bool_t>    NietBouwenInZeerMooiLandschapVerblijfsrecreatie:  [false                             ,false                              ];
	
/////////////////////////////////////////////////////////////////////////////////////// Zon ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

	// selectie van BBG-klassen die voor deze sector als ReedsBenut gelden
	attribute<string>    ReedsBenutVariant_Zon:                            [ 'Default'                        ,'Default'                          ];
	
	// Geldende restricties resp. stimuli: alleen (zeer) hard, of ook middel, zacht?
	attribute<string_t>  RestrictiesVariant_Zon:                           ['ZeerHardHardMiddel'              ,'ZeerHardHardMiddel'               ]; //fka Restricties_ref
	attribute<string_t>  StimuliVariant_Zon:                               ['HardMiddel'                      ,'HardMiddel'                       ]; //fka Stimuli_ref
	
	// Gaan nieuw gealloceerde zonnevelden samen met bestaand groen?
	attribute<bool_t>    ZonCombinerenMetBestaandGroen:                    [false                             , false                             ];
	
	// Totaal zon op land en binnenwater. Scenario:                       
	attribute<TWh>       NationaalVermogenZon2060:                         [ 29                               , 29                                ]; //kopie van 2050
	
	// Wijze waarop de claims over de regio's worden verdeeld
	attribute<string_t>  RegionaleVerdelingZon:                            ['Ruimte'                          ,'Ruimte'                           ]; //opties: Evenredig Vraag Ruimte
	
	// attribute<bool_t>    ZonIsNatuurInclusief:                             [ false                            ,false                              ];
	// attribute<string_t>  RegEnergieVerdelingZon:                           ['Ruimte'                          ,'Ruimte'                           ];


////////////////////////////////////////////////////////////////////////////////////// Wind ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

	// selectie van BBG-klassen die voor deze sector als ReedsBenut gelden
	attribute<string>    ReedsBenutVariant_Wind:                           [ 'Default'                        ,'Default'                          ];
	
	// Geldende restricties resp. stimuli: alleen (zeer) hard, of ook middel, zacht?
	attribute<string_t>  RestrictiesVariant_Wind:                          ['ZeerHardHardMiddel'              ,'ZeerHardHardMiddel'               ]; //fka Restricties_ref
	attribute<string_t>  StimuliVariant_Wind:                              ['HardMiddel'                      ,'HardMiddel'                       ]; //fka Stimuli_ref
	
	attribute<string_t>  TurbineType:                                      ['WT4000_120'                      ,'WT4000_120'                       ];
	attribute<GW>        NationaalVermogenWindOpLandEnBinnenwater2060:     [10.00                             ,10.00                              ];
	
	// Wijze waarop de claims over de regio's worden verdeeld
	attribute<string_t>  RegEnergieVerdelingWind:                          ['Ruimte'                          ,'Ruimte'                           ];
	
	// Minimale afstand tussen windturbine en een kwetsbaar object. Zie oa https://pure-energie.nl/kennisbank/hoeveel-geluid-maakt-een-windmolen
	attribute<meter>     AfstandGeluidsnorm:                               [350                               ,350                                ]; //zie oa https://pure-energie.nl/kennisbank/hoeveel-geluid-maakt-een-windmolen
	
	attribute<bool_t>    IsVoorExport:                                     [ true                             , true                              ];
	attribute<bool_t>    IsNormatief:                                      [ true                             , true                              ];
	
	container V := for_each_nedv(name, String(ID(.))+'[..]', void, .);
	
	unit<uint8> Export := select_with_org_rel(IsVoorExport)
	{
		attribute<string> name := VariantK/name[org_rel];
	}
	
	attribute<float32_t> MinimumDichtheidToenameWind               := MinimumDichtheidToenameWerken;
	attribute<float32_t> MinimumDichtheidToenameZon                := MinimumDichtheidToenameWerken;
	attribute<float32_t> MinimumDichtheidToenameVerblijfsrecreatie := MinimumDichtheidToenameWerken;
	
////////////////////////////////////////////////////////////////////////////////////// Landbouw ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
                                                                          // BusinessAsUsual                  , WBS                              
	// selectie van BBG-klassen die voor deze sector als ReedsBenut gelden
	attribute<string>    ReedsBenutVariant_Landbouw:                     ['Default'                           ,'Default'                          ];
	attribute<string_t>  RestrictiesVariant_Landbouw:                    ['Zacht'                             ,'Zacht'                            ];
	attribute<string_t>  StimuliVariant_Landbouw:                        ['Zacht'                             ,'Zacht'                            ];
	attribute<float32_t> MinimumDichtheidToenameLandbouw:                [0.0                                 ,0.0                                ];
	attribute<string>    Waterbeheeroptie:                               ['Indexation_NoDD'                   ,'Fixation_NoDD'                    ];
	attribute<string>    WinterdroogleggingK_ref:                        ['min0_1'                            ,'min0_2'                           ]; // meter tov zomerdrooglegging: Min 0.2m tov zomer in WBS; -0.1m tov zomer in BaU
	attribute<string>    ZomerdroogleggingK_ref:                         ['_0_6'                              ,'_0_2'                             ]; // m tov maaibveld
	attribute<string>    InfiltratieMaatregelK_ref:                      ['Geen_infiltratiemaatregel'         ,'Geen_infiltratiemaatregel'        ]; // AWIS, PWIS, Geen_infiltratiemaatregel
	attribute<string>    RVF_beleidsscenario:                            ['BAU'                               ,'WBS'                              ];
	attribute<string>    VerziltingsScenario:                            ['Stoom2050'                         ,'Stoom2050'                        ];
	attribute<string>    Claim_agrarisch_variant:                        ['claims_conservatief'               ,'claims_ggvee_minfractie'          ];
	attribute<bool>      ExogeenOpleggen:                                ['FALSE'                             ,'TRUE'                             ];
	// Minimale fractie van een gridcel (AllocDomain) die beschikbaar moet zijn voor bebouwing
	attribute<ha>        MinimalLandAvailability_Landbouw:               [0.60                                ,0.60                               ];
}