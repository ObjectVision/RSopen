	attribute<string>    InfiltratieMaatregelK_ref:                      ['Geen_infiltratiemaatregel'         ,'Geen_infiltratiemaatregel'        ,'Geen_infiltratiemaatregel'         ]; // AWIS, PWIS, Geen_infiltratiemaatregel
	attribute<string>    RVF_beleidsscenario:                            ['BAU'                               ,'WBS'                              ,'WBS'                               ];
	attribute<string>    VerziltingsScenario:                            ['Stoom2050'                         ,'Stoom2050'                        ,'Stoom2050'                         ];
