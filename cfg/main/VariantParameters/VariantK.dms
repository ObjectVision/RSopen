	// Wijze waarop de claims over de regio's worden verdeeld
	attribute<string_t>  RegionaleVerdelingZon:                            ['Ruimte'                            ,'Evenredig'                           ,'Evenredig'                           ]; //opties: Evenredig Vraag Ruimte

