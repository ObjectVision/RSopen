	attribute<string_t>  RestrictiesVariant_Wind:                          ['ZeerHardHardMiddel'              ,'ZeerHardHardMiddel'               ,'ZeerHardHardMiddel'               ]; //fka Restricties_ref
	attribute<string_t>  StimuliVariant_Wind:                              ['HardMiddel'                      ,'HardMiddel'                       ,'HardMiddel'                       ]; //fka Stimuli_ref
	
