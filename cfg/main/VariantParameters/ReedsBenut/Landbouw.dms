container Landbouw
{
	attribute<bool> Default (..) := BuitenPlancapaciteit/Default && BinnenPlancapaciteit/Default;

	container BinnenPlancapaciteit
	{
		attribute<bool> Default (...) := !(
			id(...) == V/overige_agrarisch_gebruik
		);
	}
	
	container BuitenPlancapaciteit
	{
		attribute<bool> Default (...) := !(
			id(...) == V/overige_agrarisch_gebruik
		);
	}
}