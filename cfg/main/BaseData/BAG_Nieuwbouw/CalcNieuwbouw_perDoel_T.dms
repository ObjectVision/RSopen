			for_each_ne(
				Vastgoed/WP2/name
				, 'Uitsmeren_T('+quote(Vastgoed/WP2/name)+', ''WP2'', ''BaseData/StartState/BAG_nieuwbouw/BAG_RecentsteJaar_pand_voorraad'', ''NieuweVBOsTussenModelenRecentJaar'')'
