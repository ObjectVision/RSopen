				,'%LocalDataDir%/RSopen/BaseData/Grondgebruik/BBG/Shares/share_lu_2012_'+Grondgebruik/CBSKlasse/gg_12K_wo_OSA/name+'.tif'
			);
		
		container J2017 := 
			for_each_neidva(
				Grondgebruik/CBSKlasse/gg_10k/name
				,'(sum(bbg2017_10m_10k == ' + string(id(Grondgebruik/CBSKlasse/gg_10k)) + '[Grondgebruik/CBSKlasse/gg_10k] ? 1f : 0f, rdc_10m/rdc_100m_rel) / 100f)'
				, 'not(all(IsNull(this))) && not(all(IsZero(this)))'
				,rdc_100m
				,float32
				,'%LocalDataDir%/RSopen/BaseData/Grondgebruik/BBG/Shares/share_lu_2017_'+Grondgebruik/CBSKlasse/gg_10k/name+'.tif'
			);
