	container Make25mgrids_modus := 
		for_each_ne(
			Grondgebruik/ReeksJaren/naam
			,'Modus25mT('+string(Grondgebruik/ReeksJaren/jaar)+')'
		);
	
	container Make100mgrids_modus := 
		for_each_ne(
			Grondgebruik/ReeksJaren/naam
			,'Modus100mT('+string(Grondgebruik/ReeksJaren/jaar)+')'
		);
	
	container MakeShare_LU_type
	{
		container J1996 :=
			for_each_neidva(
				Grondgebruik/CBSKlasse/gg_12K_wo_OSA/name
				,'(sum(bbg1996_10m_12k == ' + string(Grondgebruik/CBSKlasse/gg_12K_wo_OSA/nr_OrgEntity) + '[Grondgebruik/CBSKlasse/gg_12K] ? 1f : 0f, rdc_10m/rdc_100m_rel) / 100f)'
				, 'not(all(IsNull(this))) && not(all(IsZero(this)))'
				,rdc_100m
				,float32
				,'%LocalDataDir%/RSopen/BaseData/Grondgebruik/BBG/Shares/share_lu_1996_'+Grondgebruik/CBSKlasse/gg_12K_wo_OSA/name+'.tif'
			);
		
		container J2012 := 
			for_each_neidva(
				Grondgebruik/CBSKlasse/gg_12K_wo_OSA/name
				,'(sum(bbg2012_10m_12k == ' + string(Grondgebruik/CBSKlasse/gg_12K_wo_OSA/nr_OrgEntity) + '[Grondgebruik/CBSKlasse/gg_12K] ? 1f : 0f, rdc_10m/rdc_100m_rel) / 100f)'
				, 'not(all(IsNull(this))) && not(all(IsZero(this)))'
