		Template IsVerblijfsRecreatieSub_T
		{
			parameter<String> Year_name;
			parameter<String> Year;
			
			parameter<string> unit_str  := uint32(Year) <= 2017 ? 'BBG_Reeks_1996_2017' : 'src';
			parameter<string> vt_str    := uint32(Year) <= 2017 ? 'CBS2010Klasse' : 'CBS2020Klasse';
			parameter<string> attr_name := uint32(Year) <= 2017 ? 'Verblijfsrecreatie' : 'Verblijfsrecreatief_terrein';
			
			
			unit<UInt32> IsVerblijfsRecreatieSub := ='select_with_org_rel('+unit_str+'/CBSKlasse_rel_'+Year+' == /Classifications/Grondgebruik/'+vt_str+'/V/'+attr_name+')'
			{
				attribute<rdc_meter>  geometry (poly) := =unit_str+'/geometry[org_rel]';
				attribute<Bool>       HasLogies       := IsDefined(rlookup(id(.), point_in_polygon(union_logies_VBOs/geometry, geometry)));
			}
		}
		
		unit<UInt32> Union_logies_VBOs := ='union_unit('+asItemList('SourceData/Vastgoed/BAG/PerJaar/'+/SourceData/Vastgoed/BAG/BAG_jaren/name+'/VBO/Per_Gebruiksdoel/logies/select')+')'
