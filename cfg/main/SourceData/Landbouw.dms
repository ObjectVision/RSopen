					StorageName = "%RVF_DataDir%/Agriculture_PMeadowsVasco/yr_miscanthus.tif";
			}
			
			attribute<Ratio> yr_grass              (rdc_100m) := 1f * import/yr_grass / 100f;
			attribute<Ratio> yr_grass_reseed       (rdc_100m) := 1f * import/yr_grass_reseed / 100f;
			attribute<Ratio> yr_flowers            (rdc_100m) := 1f * import/yr_flowers / 100f;
			attribute<Ratio> yr_fruits_berry       (rdc_100m) := 1f * import/yr_fruits_berry / 100f;
			attribute<Ratio> yr_fruits_tree        (rdc_100m) := 1f * import/yr_fruits_tree / 100f;
			attribute<Ratio> yr_grains             (rdc_100m) := 1f * import/yr_grains / 100f;
			attribute<Ratio> yr_maize              (rdc_100m) := 1f * import/yr_maize / 100f;
			attribute<Ratio> yr_potatoes           (rdc_100m) := 1f * import/yr_potatoes / 100f;
			attribute<Ratio> yr_sugarbeet          (rdc_100m) := 1f * import/yr_sugarbeet / 100f;
			attribute<Ratio> yr_tree_nursery       (rdc_100m) := 1f * import/yr_tree_nursery / 100f;
			attribute<Ratio> yr_tree_nursery_other (rdc_100m) := 1f * import/yr_tree_nursery_other / 100f;
			attribute<Ratio> yr_vegetables_leaf    (rdc_100m) := 1f * import/yr_vegetables_leaf / 100f;
			attribute<Ratio> yr_vegetables_summer  (rdc_100m) := 1f * import/yr_vegetables_summer / 100f;
			attribute<Ratio> yr_vegetables_winter  (rdc_100m) := 1f * import/yr_vegetables_winter / 100f;
			attribute<Ratio> yr_miscanthus_reclass (rdc_100m) := 5f * import/yr_miscanthus_reclass / 100f;
		}
	}
	
	attribute<StallenKlasse> StallenHoofdvestiging (rdc_25m)
