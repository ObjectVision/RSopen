	attribute<Landschap/BasiskaartNatuurlijkSysteemNederland> gridcode_AdminDomain (AdminDomain) := BasiskaartNatuurlijkSysteemNederland/gridcode_rel[per_AdminDomain];
	attribute<Landschap/BasiskaartNatuurlijkSysteemNederland> gridcode_AllocDomain (AllocDomain) := BasiskaartNatuurlijkSysteemNederland/gridcode_rel[per_AllocDomain];
		
	attribute<UInt16> Huidig_GO_BNS_reclass_ZWD (AdminDomain)
	:	StorageName     = "%RSo_DataDir%/Omgeving/BasiskaartNatuurlijkSysteem/Huidig_GO_BNS_reclass_ZWD.tif"
	, 	StorageType     = "gdal.grid"
	,	StorageReadOnly = "True"
	,	DialogData      = "rdc_meter"
	,	Source          = "X:\User\bouwmana\AGP\RV_Gebiedstypen\aggregated_gebiedstypen.gdb Arno Bouwman PBL 20250908 aggregated_gebiedstypen.gdb:" 
						  "conversie Huidig_GO_BNS_reclass_ZWD naar tif in ArcMap 10.8.1 Export Data, tif, LZW"
