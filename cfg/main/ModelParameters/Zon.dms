container Zon
{
	parameter<float32> FractieVermogenHaIndienZonCombinerenMetBestaandGroen := 0.5f, Descr = "In het geval van VariantParameter ZonCombinerenMetBestaandGroen, dan komt deze fractie ten laste van het vermogen per ha hieronder. Bron: expert judgement Leo Pols (PBL, 2022/09/07)";
	
	parameter<TWh>     NationaalVermogenZon2030                             := 8.3[TWh], Descr = "Claim voor zon in 2030", Source = "KEV", URL = "";
	parameter<uur>     Vollasturen_zonnepaneel                              := 854[uur], Descr = "Standaar hoeveelheid vollasturen van een zonnepaneel";
	
	parameter<string>  Vermogen_ref                                         := 'hoog', Descr = "Uitgangspunt qua bovenstaande kentallen t.a.v. Vermogen/ha: laag, midden, hoog";
	parameter<MW_ha>   Vermogen_ha_laag                                     := 0.6[MW_ha], Descr = "blz 11, blz 9; Per hectare kunnen 0,6 tot 1,2 MW aan panelen worden geplaatst, gemiddeld wordt gerekend met 0,8 MW per hectare", URL = "https://www.rvo.nl/sites/default/files/2016/09/Grondgebonden%20Zonneparken%20-%20verkenning%20afwegingskadersmetbijlagen.pdf";
	parameter<MW_ha>   Vermogen_ha_midden                                   := 0.8[MW_ha], Descr = "blz 11, blz 9; Per hectare kunnen 0,6 tot 1,2 MW aan panelen worden geplaatst, gemiddeld wordt gerekend met 0,8 MW per hectare", URL = "https://www.rvo.nl/sites/default/files/2016/09/Grondgebonden%20Zonneparken%20-%20verkenning%20afwegingskadersmetbijlagen.pdf";
	parameter<MW_ha>   Vermogen_ha_hoog                                     := 1.2[MW_ha], Descr = "blz 11, blz 9; Per hectare kunnen 0,6 tot 1,2 MW aan panelen worden geplaatst, gemiddeld wordt gerekend met 0,8 MW per hectare", URL = "https://www.rvo.nl/sites/default/files/2016/09/Grondgebonden%20Zonneparken%20-%20verkenning%20afwegingskadersmetbijlagen.pdf";
	parameter<MW_ha>   Vermogen_ha                                          := = 'Vermogen_ha_'+Vermogen_ref, Descr = "Selectie item";

	parameter<meter2>  minimale_opp_berm                                    := 1000[meter2], Descr = "In de bepaling van de zonneladder worden potentiele locatie voor zonnepanelen geidentificeerd. Wat is de minimale oppervlakte van bermen om hiervoor te kwalificeren. Bron: expert judgement PBL";
	parameter<meter2>  minimale_opp_gras                                    := 1000[meter2], Descr = "In de bepaling van de zonneladder worden potentiele locatie voor zonnepanelen geidentificeerd. Wat is de minimale oppervlakte van gras om hiervoor te kwalificeren. Bron: expert judgement PBL";
	parameter<meter2>  minimale_opp_groen                                   := 1000[meter2], Descr = "In de bepaling van de zonneladder worden potentiele locatie voor zonnepanelen geidentificeerd. Wat is de minimale oppervlakte van groen om hiervoor te kwalificeren. Bron: expert judgement PBL";
	parameter<meter2>  minimale_opp_parkeervlak                             := 1000[meter2], Descr = "In de bepaling van de zonneladder worden potentiele locatie voor zonnepanelen geidentificeerd. Wat is de minimale oppervlakte van parkeervlakken om hiervoor te kwalificeren. Bron: expert judgement PBL";
	parameter<float32> minimale_IQ                                          := 0.05f, Descr = "In de bepaling van de zonneladder worden potentiele locatie voor zonnepanelen geidentificeerd. Wat is de minimale Isoperimetrische Quotient (een maat voor de relatie tussen de omsloten oppervlakte en de omtrek van vlakken) van vlakken om hiervoor te kwalificeren. Bron: expert judgement PBL";
	
	parameter<MW>      Vermogen_Friesland_Y2030                             := 1068[MW], Descr = "hack om een studyarea werkend te krijgen. obv nationaal 8.3 GW, regionaal verdeeld obv 'ruimte'."; 
	parameter<MW>      Vermogen_Friesland_Y2040                             := 1957[MW], Descr = "hack om een studyarea werkend te krijgen. obv nationaal 15.2 GW, regionaal verdeeld obv 'ruimte'."; 
	parameter<MW>      Vermogen_Friesland_Y2050                             := 2846[MW], Descr = "hack om een studyarea werkend te krijgen. obv nationaal 22.1 GW, regionaal verdeeld obv 'ruimte'."; 
	parameter<MW>      Vermogen_Friesland_Y2060                             := 3735[MW], Descr = "hack om een studyarea werkend te krijgen. obv nationaal 29 GW, regionaal verdeeld obv 'ruimte'."; 
}
