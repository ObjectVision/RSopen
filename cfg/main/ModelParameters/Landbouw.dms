	parameter<bool>             UseYieldReductionFromDiogoIfAvailable     := TRUE, Descr = "If available list is configured here: /Classifications/Landbouw/GewasSoortYR/HeeftDiogo_YR_map";
	parameter<float32>          Landbouw_Claim_Growth                     := 0.20f, Descr = "factor waarmee de max claims voor Landbouw worden bepaald tov prev zichtjaar";
	parameter<jaarlijks>        rentevoet                                 := 0.055[jaarlijks];
	parameter<jaar>             afschrijvingsperiode                      := 20[jaar];
	parameter<float32>          YieldReduction_NoData                     := -99999f;
	parameter<float32>          Verzilt_Areaal_Fractie                    := 0.40f, Descr = "xx% van het oppervlak van risico's 2-4 is in de toekomst niet geschikt voor de gewassen: Appelbomen, consumptieaardappelen, laanbomen, tulpen, zaaiuien en zetmeelaardappelen. Deze plekken worden random gekozen"; 
	parameter<float32>          Verzilt_OpbrengstDerving                  := 1f, Descr = "Mate van effect van verzilting op opbrengsten derving van de gewassen die daardoor worden aangetast."; 
	parameter<meter>            SuitWegingMaxAfstandN2000                 := 300[meter];
	parameter<float32>          SuitWegingFactorN2000                     := 0.2f;
	parameter<ton_ha_jaarlijks> CO2_Uitstoot_StedelijkGebruik             := 0[ton_ha_jaarlijks];
	parameter<ton_ha_jaarlijks> CO2_Uitstoot_Water                        := 0[ton_ha_jaarlijks];
	
	container claims_ggvee_minfractie // ggvee wordt voor de helft vervangen door exotoische gewassen
	{
		parameter<string> min_fractie_gg_vee := '0.90f'; // minimum percentage ggvee dat overblijft
		
		container minclaims_manual
		{
			parameter<string>   Rietteelt         := '(StartYear_minusNietBeschikbaar/gg_vee_extensief + StartYear_minusNietBeschikbaar/gg_vee + StartYear_minusNietBeschikbaar/gg_vee_intensief) * (1f - '+min_fractie_gg_vee+') * 0.35f';
			parameter<string>   Akkerbouw         := 'StartYear_minusNietBeschikbaar/Akkerbouw * 0.8f'; //extra afname om genoeg plaats te garanderen door afronding etc. 
			parameter<string>   cranberry         := '(StartYear_minusNietBeschikbaar/gg_vee_extensief + StartYear_minusNietBeschikbaar/gg_vee + StartYear_minusNietBeschikbaar/gg_vee_intensief) * (1f - '+min_fractie_gg_vee+') * 0.1f';
			parameter<string>   Boomgaard         := 'StartYear_minusNietBeschikbaar/Boomgaard';
			parameter<string>   moerasbomen       := '(StartYear_minusNietBeschikbaar/gg_vee_extensief + StartYear_minusNietBeschikbaar/gg_vee + StartYear_minusNietBeschikbaar/gg_vee_intensief) * (1f - '+min_fractie_gg_vee+') * 0.35f';
			parameter<string>   rijst             := '(StartYear_minusNietBeschikbaar/gg_vee_extensief + StartYear_minusNietBeschikbaar/gg_vee + StartYear_minusNietBeschikbaar/gg_vee_intensief) * (1f - '+min_fractie_gg_vee+') * 0.1f';
			parameter<string>   yacon             := '(StartYear_minusNietBeschikbaar/gg_vee_extensief + StartYear_minusNietBeschikbaar/gg_vee + StartYear_minusNietBeschikbaar/gg_vee_intensief) * (1f - '+min_fractie_gg_vee+') * 0.1f';
			parameter<string>   gg_vee_extensief  := 'StartYear_minusNietBeschikbaar/gg_vee_extensief * '+min_fractie_gg_vee+'';
			parameter<string>   gg_vee            := 'StartYear_minusNietBeschikbaar/gg_vee           * '+min_fractie_gg_vee+'';
			parameter<string>   gg_vee_intensief  := 'StartYear_minusNietBeschikbaar/gg_vee_intensief * '+min_fractie_gg_vee+'';
			// parameter<string>   NonLandbouw       := 'const(0[ha], RegioUnit, ha)';
		}
		
		container maxclaims_manual
		{
			parameter<string>   Rietteelt         := 'const(null_f, RegioUnit, ha)';
			parameter<string>   Akkerbouw         := 'const(null_f, RegioUnit, ha)';
			parameter<string>   Cranberry         := 'const(null_f, RegioUnit, ha)';
			parameter<string>   Boomgaard         := 'const(null_f, RegioUnit, ha)';
			parameter<string>   Moerasbomen       := 'const(null_f, RegioUnit, ha)';
			parameter<string>   Yacon             := 'const(null_f, RegioUnit, ha)';
			parameter<string>   Rijst             := 'const(null_f, RegioUnit, ha)';
			parameter<string>   gg_vee_extensief  := 'const(null_f, RegioUnit, ha)';
			parameter<string>   gg_vee            := 'const(null_f, RegioUnit, ha)';
			parameter<string>   gg_vee_intensief  := 'const(null_f, RegioUnit, ha)';
			// parameter<string>   NonLandbouw       := 'const(null_f, RegioUnit, ha)';
		}
	}
	
	container claims_conservatief
	{
		parameter<string> min_fractie_gg_vee := '0.95f'; // minimum percentage ggvee dat overblijft
		
		container minclaims_manual
		{
			parameter<string>   Rietteelt         := 'StartYear/Rietteelt';
			parameter<string>   Akkerbouw         := 'StartYear/Akkerbouw * 0.95f';
			parameter<string>   cranberry         := 'StartYear/cranberry';
			parameter<string>   Boomgaard         := 'StartYear/Boomgaard';
			parameter<string>   moerasbomen       := 'StartYear/moerasbomen';
			parameter<string>   rijst             := 'StartYear/rijst';
			parameter<string>   yacon             := 'StartYear/yacon';
			parameter<string>   gg_vee_extensief  := 'StartYear/gg_vee_extensief * '+min_fractie_gg_vee+'';
			parameter<string>   gg_vee            := 'StartYear/gg_vee           * '+min_fractie_gg_vee+'';
			parameter<string>   gg_vee_intensief  := 'StartYear/gg_vee_intensief * '+min_fractie_gg_vee+'';
		}
		
		container maxclaims_manual
		{
			parameter<string>   Rietteelt         := 'StartYear/Rietteelt * 2.0f';
			parameter<string>   Akkerbouw         := 'StartYear/Akkerbouw * 1.05f';
			parameter<string>   Cranberry         := 'StartYear/Rietteelt';
			parameter<string>   Boomgaard         := 'StartYear/Boomgaard';
			parameter<string>   Moerasbomen       := 'StartYear/Rietteelt';
			parameter<string>   Yacon             := 'StartYear/Rietteelt';
			parameter<string>   Rijst             := 'StartYear/Rietteelt';
			parameter<string>   gg_vee_extensief  := 'const(null_f, RegioUnit, ha)';
			parameter<string>   gg_vee            := 'const(null_f, RegioUnit, ha)';
			parameter<string>   gg_vee_intensief  := 'const(null_f, RegioUnit, ha)';
		}
	}
	
