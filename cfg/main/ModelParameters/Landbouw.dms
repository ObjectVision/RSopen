container Landbouw
{

	parameter<float32>          Landbouw_Claim_Growth := 0.1f, Descr = "factor waarmee de max claims voor Landbouw worden bepaald tov prev zichtjaar";
	parameter<Bool>             TotalFeedExo          := true;
	parameter<EUR_ton>          MilkPrice             := 345[EUR_ton];

	container FCE
	{	
		parameter<ratio>            gg_vee_extensief := 1.00[ratio];
		parameter<ratio>            gg_vee           := 1.10[ratio];
		parameter<ratio>            gg_vee_intensief := 1.20[ratio];
	}
	container MaxYieldGrass
	{	
		parameter<ton_ha_jaarlijks> gg_vee_extensief :=  6.419[ton_ha_jaarlijks];
		parameter<ton_ha_jaarlijks> gg_vee           :=  9.816[ton_ha_jaarlijks];
		parameter<ton_ha_jaarlijks> gg_vee_intensief := 11.164[ton_ha_jaarlijks];
	}
	container Perc_FeedDryMatter
	{	
		parameter<ratio>            gg_vee_extensief := 0.73[ratio];
		parameter<ratio>            gg_vee           := 0.70[ratio];
		parameter<ratio>            gg_vee_intensief := 0.68[ratio];
	}
	container MilkPricePremium
	{	
		parameter<EUR_ton>          gg_vee_extensief := 30.0[EUR_ton];
		parameter<EUR_ton>          gg_vee           := 00.0[EUR_ton];
		parameter<EUR_ton>          gg_vee_intensief := 00.0[EUR_ton];
	}
	
	unit<uint8> Transitiekosten := 
		for_each_nedv(
			Classifications/Actor/LandbouwK/name
			, replace('union_data(Classifications/Actor/LandbouwK,'+AsList('Transitiekosten_src/@Her@__'+Classifications/Actor/LandbouwK/name,',')+')', '@Her@', Classifications/Actor/LandbouwK/name)
			,Classifications/Actor/LandbouwK
			, EUR_ha
		), Descr = "herkomst in kolommen, bestemming in rijen";
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	container Transitiekosten_src
	{
		parameter<EUR_ha> rietteelt__rietteelt               := 0[EUR_ha];
		parameter<EUR_ha> rietteelt__akkerbouw               := 0[EUR_ha];
		parameter<EUR_ha> rietteelt__groenteteelt            := 0[EUR_ha];
		parameter<EUR_ha> rietteelt__fruitteelt              := 0[EUR_ha];
		parameter<EUR_ha> rietteelt__boomgaard               := 0[EUR_ha];
		parameter<EUR_ha> rietteelt__gg_vee_extensief        := 750[EUR_ha];
		parameter<EUR_ha> rietteelt__gg_vee                  := 750[EUR_ha];
		parameter<EUR_ha> rietteelt__gg_vee_intensief        := 750[EUR_ha];
	
		parameter<EUR_ha> gg_vee_extensief__rietteelt        := 0[EUR_ha];
		parameter<EUR_ha> gg_vee_extensief__akkerbouw        := 0[EUR_ha];
		parameter<EUR_ha> gg_vee_extensief__groenteteelt     := 0[EUR_ha];
		parameter<EUR_ha> gg_vee_extensief__fruitteelt       := 0[EUR_ha];
		parameter<EUR_ha> gg_vee_extensief__boomgaard        := 0[EUR_ha];
		parameter<EUR_ha> gg_vee_extensief__gg_vee_extensief := 0[EUR_ha];
		parameter<EUR_ha> gg_vee_extensief__gg_vee           := 750[EUR_ha];
		parameter<EUR_ha> gg_vee_extensief__gg_vee_intensief := 750[EUR_ha];
		
		parameter<EUR_ha> gg_vee__rietteelt                  := 0[EUR_ha];
		parameter<EUR_ha> gg_vee__akkerbouw                  := 0[EUR_ha];
		parameter<EUR_ha> gg_vee__groenteteelt               := 0[EUR_ha];
		parameter<EUR_ha> gg_vee__fruitteelt                 := 0[EUR_ha];
		parameter<EUR_ha> gg_vee__boomgaard                  := 0[EUR_ha];
		parameter<EUR_ha> gg_vee__gg_vee_extensief           := 2697[EUR_ha];
		parameter<EUR_ha> gg_vee__gg_vee                     := 0[EUR_ha];
		parameter<EUR_ha> gg_vee__gg_vee_intensief           := 750[EUR_ha];
		
		parameter<EUR_ha> gg_vee_intensief__rietteelt        := 0[EUR_ha];
		parameter<EUR_ha> gg_vee_intensief__akkerbouw        := 0[EUR_ha];
		parameter<EUR_ha> gg_vee_intensief__groenteteelt     := 0[EUR_ha];
		parameter<EUR_ha> gg_vee_intensief__fruitteelt       := 0[EUR_ha];
		parameter<EUR_ha> gg_vee_intensief__boomgaard        := 0[EUR_ha];
		parameter<EUR_ha> gg_vee_intensief__gg_vee_extensief := 5534[EUR_ha];
		parameter<EUR_ha> gg_vee_intensief__gg_vee           := 2837[EUR_ha];
		parameter<EUR_ha> gg_vee_intensief__gg_vee_intensief := 0[EUR_ha];
		
		parameter<EUR_ha> akkerbouw__rietteelt               := 0[EUR_ha];
		parameter<EUR_ha> akkerbouw__akkerbouw               := 0[EUR_ha];
		parameter<EUR_ha> akkerbouw__groenteteelt            := 0[EUR_ha];
		parameter<EUR_ha> akkerbouw__fruitteelt              := 0[EUR_ha];
		parameter<EUR_ha> akkerbouw__boomgaard               := 0[EUR_ha];
		parameter<EUR_ha> akkerbouw__gg_vee_extensief        := 0[EUR_ha];
		parameter<EUR_ha> akkerbouw__gg_vee                  := 0[EUR_ha];
		parameter<EUR_ha> akkerbouw__gg_vee_intensief        := 0[EUR_ha];
		
		parameter<EUR_ha> groenteteelt__rietteelt            := 0[EUR_ha];
		parameter<EUR_ha> groenteteelt__akkerbouw            := 0[EUR_ha];
		parameter<EUR_ha> groenteteelt__groenteteelt         := 0[EUR_ha];
		parameter<EUR_ha> groenteteelt__fruitteelt           := 0[EUR_ha];
		parameter<EUR_ha> groenteteelt__boomgaard            := 0[EUR_ha];
		parameter<EUR_ha> groenteteelt__gg_vee_extensief     := 0[EUR_ha];
		parameter<EUR_ha> groenteteelt__gg_vee               := 0[EUR_ha];
		parameter<EUR_ha> groenteteelt__gg_vee_intensief     := 0[EUR_ha];
		
		parameter<EUR_ha> fruitteelt__rietteelt              := 0[EUR_ha];
		parameter<EUR_ha> fruitteelt__akkerbouw              := 0[EUR_ha];
		parameter<EUR_ha> fruitteelt__groenteteelt           := 0[EUR_ha];
		parameter<EUR_ha> fruitteelt__fruitteelt             := 0[EUR_ha];
		parameter<EUR_ha> fruitteelt__boomgaard              := 0[EUR_ha];
		parameter<EUR_ha> fruitteelt__gg_vee_extensief       := 0[EUR_ha];
		parameter<EUR_ha> fruitteelt__gg_vee                 := 0[EUR_ha];
		parameter<EUR_ha> fruitteelt__gg_vee_intensief       := 0[EUR_ha];
		
		parameter<EUR_ha> boomgaard__rietteelt               := 0[EUR_ha];
		parameter<EUR_ha> boomgaard__akkerbouw               := 0[EUR_ha];
		parameter<EUR_ha> boomgaard__groenteteelt            := 0[EUR_ha];
		parameter<EUR_ha> boomgaard__fruitteelt              := 0[EUR_ha];
		parameter<EUR_ha> boomgaard__boomgaard               := 0[EUR_ha];
		parameter<EUR_ha> boomgaard__gg_vee_extensief        := 0[EUR_ha];
		parameter<EUR_ha> boomgaard__gg_vee                  := 0[EUR_ha];
		parameter<EUR_ha> boomgaard__gg_vee_intensief        := 0[EUR_ha];
	}
}