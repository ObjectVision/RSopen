container Landbouw
{
	parameter<bool>             UseUndefinedMaxClaims                     := FALSE;
	parameter<bool>             UseYieldReductionFromDeltaresIfAvailable  := TRUE, Descr = "If available list is configured here: /Classifications/Landbouw/GewasSoortYR/HeeftDeltares_YR_map";
	parameter<bool>             UseYieldReductionFromDiogoIfAvailable     := TRUE, Descr = "If available list is configured here: /Classifications/Landbouw/GewasSoortYR/HeeftDiogo_YR_map";
	parameter<float32>          Landbouw_Claim_Growth                     := 10f, Descr = "factor waarmee de max claims voor Landbouw worden bepaald tov prev zichtjaar";
	parameter<jaarlijks>        rentevoet                                 := 0.055[jaarlijks];
	parameter<jaar>             afschrijvingsperiode                      := 20[jaar];
	parameter<float32>          YieldReduction_NoData                     := -99999f;
	
	unit<uint8> Transitiekosten := 
		for_each_nedv(
			Classifications/Actor/LandbouwK/name
			, replace('union_data(Classifications/Actor/LandbouwK,'+AsList('Transitiekosten_src/@Her@__'+Classifications/Actor/LandbouwK/name,',')+')', '@Her@', Classifications/Actor/LandbouwK/name)
			,Classifications/Actor/LandbouwK
			, EUR_ha
		), Descr = "herkomst in kolommen, bestemming in rijen";
	
	container Dairy 
	{
		parameter<Bool>             TotalFeedExo                 := true;
		parameter<EUR_ton>          MilkPrice                    := 345[EUR_ton];
		parameter<EUR_ton>          Holding_costs                := 157.1[EUR_ton]; // wat zijn dit precies (paper vasco)? Updaten of speciferen per intensiteitsklasse? Te speciferen per soc. econ. scenario of voor gevoeligheidsanalyse?
		parameter<EUR_ton>          Manure_revenue               := 0[EUR_ton];
		
		parameter<ratio>            feed_composition_share_grass := 0.75[ratio]; //0.6
		parameter<ratio>            feed_composition_share_mais  := 0.25[ratio]; //0.2
	}
	
