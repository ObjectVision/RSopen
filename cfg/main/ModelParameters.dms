container ModelParameters : using = "Units;Geography;SourceData/RegioIndelingen"
{
	parameter<String>    StudyArea                     := 'Nederland', Descr = "Welk gebied moet het model beslaan. Heel Nederland, of een kleiner studiegebied. Opties: Nederland Friesland Noord_Holland Utrecht. De mogelijke opties zijn gedefinieerd in Geography/StudyAreas. De studiegebied keuze heeft natuurlijk effect op welke cellen er beschouwd worden. Deze waarde wordt ook als suffix in alle ontkoppelde/resultaat bestanden verwerkt."; 
	parameter<Bool>      AlleenEindjaar                := =replace_value(expand(., '%env:AlleenEindjaar%'), 'env:AlleenEindjaar', 'TRUE'), Descr = "In één keer de stand in het laatste zichtjaar evalueren of in tussenstappen, per zichtjaar: TRUE, FALSE. TRUE, betekent dus alle tussenliggende zichtjaren overslaan en daarmee ook de padafhankelijkheid. Dit kan best wat effect hebben op de uiteindelijke resultaten.";
	
	// Gebruik maken van reeds berekende en opgeslagen (ontkoppelde) allocatieresultaten (post-allocatie en (dus) casus specifiek)
	parameter<Bool>      BaseDataOntkoppeld            := TRUE, Descr = "Gebruik maken van reeds berekende en opgeslagen (ontkoppelde) tussenresultaten (pre-allocatie, generiek): TRUE, FALSE";
	parameter<Bool>      VariantDataOntkoppeld         := TRUE, Descr = "Gebruik maken van reeds berekende en opgeslagen (ontkoppelde) tussenresultaten (pre-allocatie, Variant-specifiek): TRUE FALSE";
	parameter<Bool>      StandAllocatieOntkoppeld      := TRUE, Descr = "Wil je ontkoppelde model (stand) resultaten gebruiken";
	parameter<Bool>      BAG_WP5_rel_Ontkoppeld        := TRUE, Descr = "Voor debug doeleinden; tijdens het debuggen is het onhandig om telkens dit te laten uitrekenen. Normaal moet dit dus op FALSE stsaan. Het wordt overigens slechts in de LD ontkoppeld.";
	
	parameter<YearRange> Model_StartYear               := 2023[YearRange], Descr = "Actueel/recent jaar met dubbele functuie: 1) centraal uitgangspunt voor de allocatie; 2) grondslag voor de verhouding tussen de verschillende lagen, bijv Woningen of banen per hectare.", IntegrityCheck = "This < 2030[YearRange]"; //check om ervoor te zorgen dat dynamische set van zichtjaren blijft werken /Classifications/Time/ModelJaar
	parameter<YearRange> Model_FinalYear               := 2050[YearRange], Descr = "Laatste jaar waarvoor standgegevens worden berekend in de allocatieprocedure", IntegrityCheck = "This > 2030[YearRange]"; //check om ervoor te zorgen dat dynamische set van zichtjaren blijft werken /Classifications/Time/ModelJaar

	parameter<UInt8>     Default_NrOfIters             := 10b, Descr = "Aantal iteraties in de allocatie. Iteraties zijn  nodig om niet vervulde restclaims in een iteratie in een volgende iteratie te vervullen";
	parameter<UInt8>     Default_NrOfSequences         := 2b, Descr = "Aantal sequences in de allocatie. Sequences zijn nodig om verdrongen wooneenheden, banen etc in een sequentie in een volgende sequentie te heralloceren, voor zover de verdringing restclaims opleverede. Dit moet minimaal 2 zijn.", IntegrityCheck = "This >= 2b"; 

	// Jaren/Data waarvoor recente waarnemingen beschikbaar zijn. t.b.v. het inlezen ervan.
	parameter<UInt32>    BAG_RecentYear                :=  2025, Descr = "Wat is het meest recent beschikbare jaar van de BAG";
	parameter<String>    BAG_file_date                 := '20250710', Descr = "Welke bestandsdatum van BAG moet worden gebruikt";
	parameter<UInt32>    LISA_FileYear                 :=  2023, Descr = "Wat is het bestandsjaar van de LISA dataset";
	parameter<UInt32>    LISA_StartYear                :=  2023, Descr = "Welk jaar van LISA moet worden gebruikt", IntegrityCheck = "LISA_StartYear <= LISA_FileYear"; //evident dat lisa startjaar kleiner over gelijk moet zijn aan het filejaar. Het zijn immers geen prognses.
	parameter<String>    BBG_Year                      := '2020', Descr = "Welk jaar van BBG moet worden gebruikt";
	parameter<String>    BBG_Verblijfsrecreatie_Year   := '2017', Descr = "Om Verblijfsrecreatie trends te bepalen moet een BBG jaar gekozen worden.";
	parameter<String>    BGT_file_date                 := '20250305', Descr = "Welke bestandsdatum van de Basisregistratie Grootschalige Topografie moet worden gebruikt"; 
	parameter<String>    BRT_file_date                 := '20250318', Descr = "Welke bestandsdatum van de Basisregistratie Topografie moet worden gebruikt";
	parameter<String>    IBIS_Year                     := '2023', Descr = "Welk jaar van IBIS moet worden gebruikt";
	parameter<String>    NVM_filedate                  := '20250320', Descr = "Welke bestandsdatum van de NVM-coefficienen moet worden gebruikt";
	parameter<String>    NVM_coeff_Year                := '2022', Descr = "Welk jaar moet als prijsniveau worden gehanteerd uit de hedonische prijsanalyse";
	parameter<String>    NWB_file_date                 := '20211001', Descr = "Welke bestandsdatum van het Nationaal Wegenbestand moet worden gebruikt";
	parameter<String>    LGN_Year                      := '2022', Descr = "Welke bestandsdatum van het Landelijk Grondgebruiksbestand Nederland moet worden gebruikt";
	parameter<String>    TIGRIS_filedate               := '20241223_Run29_correctie', Descr = "Welke versie van de TIGRIS-claims moet worden gebruikt (relevant voor wonen/werken)"; 
	parameter<String>    Restricties_filedate          := '2025-05-20_15-21-16', Descr = "Welke bestandsdatum van de Ruimtelijke Restricities moet worden gebruikt";
	parameter<String>    Stimuli_filedate              := '2025-02-17_11-52-29', Descr = "Welke bestandsdatum van de Stimuli moet worden gebruikt";
	parameter<String>    PlanCapaciteit_filedate       := '2025-05-22_09-34-37', Descr = "Welke bestandsdatum van de Plan Capaciteiten moet worden gebruikt";
	parameter<Bool>      UsePrivPlanCapaciteit         := TRUE, Descr = "Er zijn publiek en niet-publiek beschikbare plancapaciteiten. Wil je die niet-publieke gebruiken?";

	#include<Wonen.dms>
	#include<Werken.dms>
	#include<Verblijfsrecreatie.dms>
	#include<Zon.dms>
	#include<Wind.dms>
	#include<Landbouw.dms>
	
	#include<Advanced.dms>
}