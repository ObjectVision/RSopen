container Gras_zonder_herinzaai
{
	parameter<Classifications/Omgeving/GewasSoortYR> Gewas: expr = "Classifications/Omgeving/GewasSoortYR/V/Gras_zonder_herinzaai";

	attribute<ratio> rotation(domain_grid):
		Expr = "switch("
			   "case(Omgeving/NatuurLandschap/Bodem_HELP >= value (1, Classifications/Omgeving/BodemKlasse14k) && Omgeving/NatuurLandschap/Bodem_HELP <= value (5, Classifications/Omgeving/BodemKlasse14k),"
					"value ( 1.0, ratio))," //0.801
			   "case(Omgeving/NatuurLandschap/Bodem_HELP >= value (6, Classifications/Omgeving/BodemKlasse14k) && Omgeving/NatuurLandschap/Bodem_HELP <= value (14, Classifications/Omgeving/BodemKlasse14k),"
					"value ( 1.0, ratio))," //0.630
			   "value ( 0.0, ratio)) ";

	attribute<Eur_Ha_jaarlijks> production_costs(rdc_100m): Expr = "value (774.0, Eur_Ha_jaarlijks) * rotation";   // updaten of speciferen per intensiteitsklasse? // wat zijn dit precies (paper vasco)?

	attribute<float32> yr (domain_grid): expr = "='factoren/fysiek/YR/Average/'+Classifications/Omgeving/GewasSoortYR/name[gewas]";
	
	attribute<ton_ha_jaarlijks> DM_yield (domain_grid): Expr = "value (MaxYieldGrass + DDGrassYield, ton_ha_jaarlijks) * (1.0[float32] - yr) * rotation";
	
	parameter<EUR_ton> feeding_costs: Expr = " value (16.0, EUR_ton)"; // updaten of speciferen per intensiteitsklasse?    // wat zijn dit precies (paper vasco)?
}